{
	"compilerOptions": {
		"target": "es5",
		"module": "commonjs",
		"sourceMap": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"rootDir": ".",
		"outDir": "../../../target/cjs",
		"moduleResolution": "node"
	},
	"exclude": [
		"node_modules"
	]
}
